<div class="container">
    <form action="/emp/final-update-emp" method="post">

   <input type="hidden" name="id" value="{{emp.id}}"/>
<div class="form-group">
  <fieldset>
    <label class="form-label mt-4" >Name</label>
    <input class="form-control"name="fullName" value="{{emp.fullName}}" type="text">
  </fieldset>
  
</div>
<div class="form-group">
  <fieldset>
    <label class="form-label mt-4" >Email</label>
    <input class="form-control" name="email" value="{{emp.email}}" type="text">
  </fieldset>
  
</div>
<div class="form-group">
  <fieldset>
    <label class="form-label mt-4" >City</label>
    <input class="form-control" name="city" value="{{emp.city}}" type="text">
  </fieldset>
  
</div>
<div class="form-group">
  <fieldset>
    <label class="form-label mt-4" >PhoneNo</label>
    <input class="form-control" name="phone" value="{{emp.phone}}" type="text">
  </fieldset>
  
</div>
<br>

<input type="submit" class="btn btn-info btn-lg">
 </form>
</div>
